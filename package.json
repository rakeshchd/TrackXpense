{
  "name": "trackxpense",
  "version": "1.0.0",
  "description": "TrackXpense - A user-friendly finance management website for students",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
